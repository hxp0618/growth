        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7638a865-4a4b-413f-8e27-1f40f566d73f"
      }
    },
    "runtimeVersion": {
